{
  "extends": "tslint-config-airbnb",
  "rulesDirectory": "ts-rules",
  "rules": {
    "spec-defocus": true,
    "no-underscore-property": true,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [true, "spaces", 4],
    "label-position": true,
    "max-line-length": [
      true,
      120
    ],
    "prefer-const": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "no-trailing-whitespace": [true, "ignore-template-strings", "ignore-comments", "ignore-blank-lines"],
    "no-unused-expression": true,
    "no-var-keyword": true,
    "new-parens": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": ["check-format", "allow-leading-underscore"],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "align": [true, "parameters", "statements", "members", "elements"],
    "no-consecutive-blank-lines": true,
    "interface-over-type-literal": true,
    "no-inferrable-types": [true, "ignore-params"],
    "no-trailing-whitespace": [
      true,
      "ignore-template-strings",
      "ignore-comments",
      "ignore-blank-lines"
    ],
    "no-console": [
      true,
      "log",
      "debug",
      "time",
      "timeEnd",
      "trace"
    ],
    "ter-indent": [
      true,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "prefer-array-literal": false,
    "import-name": false,
    "object-curly-spacing": [
      false
    ],
    "function-name": [
      true,
      {
        "function-regex": "^[A-Za-z$][\\w\\d]+$",
        "method-regex": "^[a-z$][\\w\\d]+$",
        "private-method-regex": "^[a-z$][\\w\\d]+$",
        "protected-method-regex": "^[a-z$][\\w\\d]+$",
        "static-method-regex": "^[a-z$][\\w\\d]+$"
      }
    ],
    "no-this-assignment": [false, "allow-destructuring"],
    "ter-prefer-arrow-callback": [false],
    "ter-arrow-parens": [false],
    "no-duplicate-imports": false,
    "void-zero": true
  }
}
